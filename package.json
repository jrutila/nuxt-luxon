{
  "name": "nuxt-luxon",
  "version": "1.2.0",
  "description": "Easy DateTime formatting & parsing in Vue using Luxon",
  "author": "Donald Shtjefni <dnld.sht@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dnldsht/nuxt-luxon.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "nuxt",
    "nuxt3",
    "luxon",
    "date",
    "datetime",
    "time",
    "format",
    "parse"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "coverage": "vitest run --coverage",
    "docs:dev": "pnpm -C docs docs:dev",
    "docs:build": "pnpm -C docs docs:build",
    "docs:preview": "pnpm -C docs docs:preview"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.2",
    "luxon": "^3.6.1"
  },
  "optionalDependencies": {
    "@nuxtjs/i18n": "^10.1.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.4.0",
    "@nuxt/eslint": "1.3.0",
    "@nuxt/eslint-config": "^1.3.0",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.2",
    "@nuxt/test-utils": "^3.18.0",
    "@types/luxon": "^3.6.2",
    "@types/node": "latest",
    "@vitest/coverage-v8": "3.1.3",
    "changelogen": "^0.6.1",
    "defu": "^6.1.4",
    "eslint": "^9.26.0",
    "nuxt": "^3.17.2",
    "typescript": "~5.8.3",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.2.10"
  }
}
